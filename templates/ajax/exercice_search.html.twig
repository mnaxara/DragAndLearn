        <th>Libelle</th>
        <th>Aide</th>
        <th>Instruction</th>
    </tr>
    </thead>
    <tbody>
    {% for exercice in exercices %}

        <tr class="border-bottom border-dark text-center">
            <td>{{ exercice.libelle }}</td>
